<PolarisList as |list|>
  {{#each model as |testPage|}}
    <list.item>
      <LinkTo @route={{testPage.path}}>
        {{testPage.text}}
      </LinkTo>
    </list.item>
  {{/each}}
</PolarisList>

<PolarisLink
  @url="https://github.com/smile-io/ember-polaris"
  @external={{true}}
  @text="Ember Polaris on GitHub"
/>