{{#wrapper-element
  class="Polaris-ActionList"
  tagName=(if hasMultipleSections "ul" "div")
}}
  {{#each finalSections as |section|}}
    {{polaris-action-list/section
      section=section
      hasMultipleSections=hasMultipleSections
      actionRole=actionRole
      onActionAnyItem=(action onActionAnyItem)
    }}
  {{/each}}
{{/wrapper-element}}
